# template for .wrt.conf
[default]
SERVER: <ip:[port]>
USERNAME: <username>
PASSWORD: <password>
PROJECT_NAME: <name>
PROTOCOL: http[s]
STORAGE: <database|swift>

# provide labels and shell commands that output the values
[tags]
REVISION: <shell command that outputs unique revision specifier>

[swift]
AUTH_URL: <url>
AUTH_VERSION: <1|2|3>
TENANT: <tenant>
USER: <username>
KEY: <password>
# 30 days
EXPIRE_SECONDS: 2592000
